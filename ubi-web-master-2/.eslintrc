{
  "plugins": ["jest", "unused-imports", "import", "simple-import-sort"],
  "extends": ["react-app", "plugin:jsx-a11y/recommended"],
  "settings": {
    "import/extensions": [".js", ".jsx"]
  },
  "globals": {
    "AfterPay": true
  },
  "rules": {
    "jsx-a11y/no-onchange": 1,
    "jsx-a11y/click-events-have-key-events": 1,
    "jsx-a11y/media-has-caption": 1,
    "import/extensions": 1,
    "import/no-named-as-default": 0,
    "import/no-extraneous-dependencies": 1,
    "import/first": 1,
    "import/newline-after-import": 1,
    "import/no-duplicates": 2,
    "import/no-unresolved": 1,
    "no-unused-vars": 0,
    "simple-import-sort/imports": 1,
    "simple-import-sort/exports": 1,
    "unused-imports/no-unused-imports": 1,
    "unused-imports/no-unused-vars": [
      1,
      { "vars": "all", "varsIgnorePattern": "^_", "args": "after-used", "argsIgnorePattern": "^_" }
    ],
    "indent": [
      "error",
      2,
      {
        "ignoredNodes": ["TemplateLiteral"],
        "SwitchCase": 1,
        "VariableDeclarator": {
          "var": 2,
          "let": 2,
          "const": 3
        },
        "MemberExpression": 1,
        "FunctionExpression": {
          "body": 1
        },
        "ArrayExpression": 1,
        "ObjectExpression": 1,
        "ImportDeclaration": 1
      }
    ],
    "no-inner-declarations": 2,
    "quotes": ["error", "single", { "allowTemplateLiterals": true }],
    "jsx-quotes": ["error", "prefer-single"],

    "prefer-destructuring": 0,
    "react/destructuring-assignment": 0,
    "react/button-has-type": 0,
    "react/no-access-state-in-setstate": 0,
    "react/jsx-max-props-per-line": [1, { "when": "always" }],
    "react-hooks/exhaustive-deps": 0,
    "react/forbid-foreign-prop-types": 0,
    "eqeqeq": ["error", "always"],
    "comma-dangle": ["error", "never"],
    "semi-style": ["error", "last"],
    "space-before-function-paren": ["error", "never"],
    "keyword-spacing": [
      "error",
      {
        "overrides": {
          "if": { "after": false, "before": false }
        }
      }
    ],
    "no-useless-return": 2,
    "no-return-assign": 0,
    "no-dupe-keys": 2,
    "wrap-iife": 0,
    "no-floating-decimal": 0,
    "no-constant-condition": 0,
    "no-sequences": 2,
    "no-cond-assign": 0,
    "no-restricted-properties": 0,
    "generator-star-spacing": 0,
    "operator-assignment": 0,
    "operator-linebreak": ["error", "after"],
    "react/jsx-filename-extension": 0,
    "react/jsx-no-undef": 1,
    "no-mixed-spaces-and-tabs": 2,
    "no-tabs": 2,
    "no-console": 2,
    "no-unreachable": 2,
    "curly": ["error", "all"],
    "no-undef": 1,
    "max-len": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "padded-blocks": 0,
    "react/prop-types": 0,
    "array-callback-return": 0,
    "react/sort-comp": 0,
    "class-methods-use-this": 0,
    "one-var": 0,
    "prefer-const": 0,
    "react/jsx-closing-bracket-location": [2, "tag-aligned"],
    "brace-style": ["error", "stroustrup"],
    "no-param-reassign": ["error", { "props": true, "ignorePropertyModificationsFor": ["elem", "index"] }],

    "space-in-parens": ["error", "always"],
    "no-multi-spaces": ["error", { "exceptions": { "VariableDeclarator": true } }],
    "react/require-default-props": 0,
    "semi": 1,
    "react/prefer-stateless-function": 0,
    "react/jsx-indent": [2, 2],
    "no-trailing-spaces": 2,
    "no-restricted-syntax": 0,
    "react/jsx-indent-props": 0,
    "arrow-parens": 0,
    "object-curly-spacing": ["error", "always"],
    "no-multiple-empty-lines": 0,
    "space-before-blocks": ["error", "never"],
    "spaced-comment": ["error", "always", { "line": { "exceptions": ["-"] } }],
    "arrow-body-style": 0,
    "react/jsx-pascal-case": 0,
    "space-i": 0,
    "react/jsx-curly-spacing": [2, { "when": "always", "children": { "when": "always" } }],
    "no-empty": [2, { "allowEmptyCatch": true }],
    "prefer-template": 0,
    "arrow-spacing": 0,
    "react/no-array-index-key": 0,
    "no-else-return": 0,
    "no-lonely-if": 2,
    "no-whitespace-before-property": 0,
    "react/jsx/equals-spacing": 0,
    "comma-spacing": ["error", { "before": false, "after": true }],
    "no-useless-constructor": 2,
    "space-infix-ops": ["error", { "int32Hint": false }],
    "no-template-curly-in-string": 0,
    "react/jsx-tag-spacing": 0,
    "react/jsx-first-prop-new-line": 0,
    "react/jsx-equals-spacing": [2, "never"],
    "key-spacing": 0,
    "quote-props": 0,
    "no-plusplus": 0,
    "no-case-declarations": 0,
    "computed-property-spacing": 0,
    "react/jsx-boolean-value": 0,
    "linebreak-style": 0,
    "block-spacing": 0,
    "import/no-mutable-exports": 0,
    "array-bracket-spacing": ["error", "never"],
    "dot-notation": 0,
    "no-useless-computed-key": 0,
    "no-var": 0,
    "no-use-before-define": 0,
    "react/no-unused-prop-types": 0,
    "no-extra-semi": 2,
    "no-undef-init": 0,
    "object-shorthand": 0,
    "no-unneeded-ternary": 2,
    "template-curly-spacing": ["off"],
    "camelcase": 0,
    "space-unary-ops": 0,
    "react-self-closing-comp": 0,
    "vars-on-top": 0,
    "react/no-did-update-set-state": 0,
    "react/self-closing-comp": 0,
    "func-call-spacing": ["error", "never"],
    "no-spaced-func": 0,
    "semi-spacing": 0,
    "no-shadow": 0,
    "react/jsx-no-bind": [
      2,
      {
        "ignoreDOMComponents": false,
        "ignoreRefs": true,
        "allowArrowFunctions": true,
        "allowFunctions": false,
        "allowBind": false
      }
    ],
    "no-unused-expressions": 0,
    "one-var-declaration-per-line": 0,
    "default-case": 0,
    "no-fallthrough": 0,
    "no-mixed-operators": 0,
    "radix": 2,
    "no-useless-escape": 0,
    "block-scoped-var": 0,
    "no-redeclare": 0,
    "func-names": 0,
    "no-confusing-arrow": 0,
    "no-nested-ternary": 0,
    "no-lone-blocks": 0,
    "eol-last": 0,
    "import/prefer-default-export": 0,
    "prefer-arrow-callback": 0,
    "react/no-did-mount-set-state": [2, "disallow-in-func"],
    "react/jsx-wrap-multilines": [
      2,
      {
        "return": "parens-new-line",
        "assignment": "parens-new-line",
        "declaration": "parens-new-line",
        "arrow": "parens-new-line",
        "condition": "parens-new-line",
        "logical": "ignore",
        "prop": "ignore"
      }
    ],
    "require-yield": 0,
    "no-throw-literal": 0,
    "no-unused-labels": 0,
    "no-labels": 0,
    "no-irregular-whitespace": 0,
    "comma-style": 0,
    "no-unexpected-multiline": 0,
    "no-useless-concat": 2,
    "newline-per-chained-call": 0,
    "react/no-danger": 0,
    "react/forbid-prop-types": 0,
    "jest/no-disabled-tests": "error",
    "jest/no-focused-tests": "error",
    "jest/no-test-prefixes": "error"
  },
  "overrides": [
    {
      "files": ["*.stories.js"],
      "rules": {
        "import/no-anonymous-default-export": "off"
      }
    }
  ]
}
